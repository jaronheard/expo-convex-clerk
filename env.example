# Example environment variables for Expo + Convex + Clerk Template
# Rename this file to .env.local and fill in your values

# Convex
EXPO_PUBLIC_CONVEX_URL=

# Clerk
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=
EXPO_PUBLIC_CLERK_FRONTEND_API_URL=

# OneSignal
EXPO_PUBLIC_ONESIGNAL_APP_ID=

# PostHog
EXPO_PUBLIC_POSTHOG_API_KEY=

# AppsFlyer
EXPO_PUBLIC_APPSFLYER_DEV_KEY=
EXPO_PUBLIC_APPSFLYER_APP_ID=

# Sentry
EXPO_PUBLIC_SENTRY_DSN=
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# Optional: set the app variant when running locally
APP_VARIANT=

# Optional: OpenAI
OPENAI_API_KEY=

# Optional: override to "ios" or "android" when running platform-specific builds
EXPO_OS=
