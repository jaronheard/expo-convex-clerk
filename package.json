{
  "name": "expo-convex-clerk-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "mcp": "npx @agentdeskai/browser-tools-server@1.2.0",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "APP_VARIANT=development npx expo start",
    "build": "pnpm run deploy:convex",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "eas:build.dev.device.ios": "dotenv -e .env.local -- eas build --profile development-device --platform ios",
    "eas:build.dev.device": "dotenv -e .env.local -- eas build --profile development-device --platform ios --platform android",
    "eas:build.dev.sim.android": "dotenv -e .env.local -- eas build --profile development --platform android",
    "eas:build.dev.sim.ios": "dotenv -e .env.local -- eas build --profile development --platform ios",
    "eas:build.dev.sim": "dotenv -e .env.local -- eas build --profile development --platform ios --platform android",
    "eas:build.prod.android": "dotenv -e .env.production -- eas build --profile production --platform android",
    "eas:build.prod.ios": "dotenv -e .env.production -- eas build --profile production --platform ios",
    "eas:install.sim.latest.android": "dotenv -e .env.production -- eas build:run -p android --latest",
    "eas:install.sim.latest.ios": "dotenv -e .env.production -- eas build:run -p ios --latest",
    "eas:metadata:pull": "dotenv -e .env.production -- eas metadata:pull",
    "eas:metadata:push": "dotenv -e .env.production -- eas metadata:push",
    "eas:submit.prod.ios": "dotenv -e .env.production -- eas submit --platform ios",
    "eas:update.preview": "dotenv -e .env.local -- eas update --branch preview && npx sentry-expo-upload-sourcemaps dist",
    "eas:update.prod": "dotenv -e .env.production -- eas update --branch production --clear-cache && npx sentry-expo-upload-sourcemaps dist",
    "deploy:convex": "npx convex deploy --prod --yes"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.11.5",
    "@clerk/clerk-react": "^5.31.4",
    "@convex-dev/workflow": "^0.2.4",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.4",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@rn-primitives/avatar": "^1.2.0",
    "@rn-primitives/checkbox": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@sentry/react-native": "^6.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.24.1",
    "convex-helpers": "^0.1.87",
    "dotenv-cli": "^8.0.0",
    "expo": "~53.0.9",
    "expo-application": "~6.1.4",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-router": "~5.0.6",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-updates": "~0.28.12",
    "expo-web-browser": "~14.1.6",
    "lucide-react-native": "^0.511.0",
    "nativewind": "^4.1.23",
    "onesignal-expo-plugin": "^2.0.3",
    "posthog-react-native": "^3.15.3",
    "posthog-react-native-session-replay": "^1.0.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.3",
    "react-native": "0.79.2",
    "react-native-appsflyer": "^6.16.2",
    "react-native-error-boundary": "^2.0.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-onesignal": "^5.2.11",
    "react-native-purchases": "^8.10.1",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
